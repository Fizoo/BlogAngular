{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
    ],
 "dataGroups": [
   {
     "name": "firebase-posts",
     "urls": [
       "https://angular-blog-afd86-default-rtdb.firebaseio.com/**"
     ],
     "cacheConfig": {
       "maxSize": 10,
       "maxAge": "3600"
     }
   }
 ]
}
